<div class='container'>
  <div class='row'>
    <!-- 検索対象に応じて表示内容を変更 -->
    <% if @search_target == "User" %>
      <!-- ユーザ一覧テーブル -->
      <h2>Users search for '<%= @search_name %>'</h2>
      <%= render 'users/users_table', users: @results %>
    <% else %>
      <!-- 書籍一覧テーブル -->
      <h2>Books search for '<%= @search_name %>'</h2>
      <%= render 'books/books_table', books: @results %>
    <% end %>
  </div>
</div>